msgstr ""

#: ..\python_docs\NVDA-Add-on-Development-Guide.py:763
msgid "\t\t\tui.message(lineNum)"
msgstr ""

#: ..\python_docs\NVDA-Add-on-Development-Guide.py:766
msgid "\t\t\t\"kb:NVDA+S\":\"sayLineNumber\""
