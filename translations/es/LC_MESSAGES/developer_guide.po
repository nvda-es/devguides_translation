msgid "    import api\n"
msgstr "    import api\n"

#: ..\python_docs\developer_guide.py:378
msgid ""
"    \t\tdescription=_(\"Announces the window class name of the current focus "
"object\"),\n"
msgstr ""
"    \t\tdescription=_(\"Announces the window class name of the current focus "
"object\"),\n"

#: ..\python_docs\developer_guide.py:380
msgid "    \t\tgesture=\"kb:NVDA+leftArrow\"\n"
msgstr "    \t\tgesture=\"kb:NVDA+leftArrow\"\n"

#: ..\python_docs\developer_guide.py:384
msgid "    \tdef script_announceWindowClassName(self, gesture):\n"
msgstr "    \tdef script_announceWindowClassName(self, gesture):\n"

#: ..\python_docs\developer_guide.py:386 ..\python_docs\developer_guide.py:406
msgid "    \t\tfocusObj = api.getFocusObject()\n"
msgstr "    \t\tfocusObj = api.getFocusObject()\n"

#: ..\python_docs\developer_guide.py:388 ..\python_docs\developer_guide.py:408
msgid "    \t\tname = focusObj.name\n"
msgstr "    \t\tname = focusObj.name\n"

#: ..\python_docs\developer_guide.py:390
msgid "    \t\twindowClassName = focusObj.windowClassName\n"
msgstr "    \t\twindowClassName = focusObj.windowClassName\n"

#: ..\python_docs\developer_guide.py:392
