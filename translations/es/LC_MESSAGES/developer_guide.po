#: ..\python_docs\developer_guide.py:400
msgid "    \t\tgesture=\"kb:NVDA+rightArrow\"\n"
msgstr "    \t\tgesture=\"kb:NVDA+rightArrow\"\n"

#: ..\python_docs\developer_guide.py:404
msgid "    \tdef script_announceWindowControlID(self, gesture):\n"
msgstr "    \tdef script_announceWindowControlID(self, gesture):\n"

